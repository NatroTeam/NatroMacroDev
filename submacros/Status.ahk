dispatch_handler(self, event, data) {
    switch event, 0 {
        case "INTERACTION_CREATE":
            interaction := Discord.Interaction(self, data)
            switch interaction.type, 0 {
                case 2: ;? Slash Command
                    switch interaction.data.name, 0 {
                        case "help":
                            mode := interaction.getStringOption("command").unwrap_or("")
                            switch mode, 0 {
                                case "Advanced":
                                    interaction.Reply({
                                        embeds: [
                                            EmbedBuilder()
                                            .set_title("Advanced Commands")
                                            .set_color(0x7380FF)
                                            .add_fields({
                                                name: "/settings set [setting] [value]",
                                                value: "Set a setting for the macro. Use `/help [settings]` to see all settings",
                                                inline: true
                                            }, {
                                                name: "/settings get [settings]",
                                                value: "Get a setting for the macro. Use `/help [settings]` to see all settings",
                                                inline: true
                                            }, {
                                                name: "/send [keys]",
                                                value: "Uses AHK's ``Send`` function. See [AHK Docs](<https://www.autohotkey.com/docs/v2/lib/Send.htm>)",
                                                inline: true
                                            }, {
                                                name: "/click [x] [y] [amount] [mode]",
                                                value: "Click at the specified coordinates. While using the specified mode",
                                                inline: true
                                            }, {
                                                name: '/upload [path]',
                                                value: 'Upload a file to Discord',
                                                inline: true
                                            }, {
                                                name: '/download [file] [path]',
                                                value: 'Download a file from Discord',
                                                inline: true
                                            }, {
                                                name: '/activate [window]',
                                                value: 'Activate a window by name',
                                                inline: true
                                            }, {
                                                name: '/minimize [window]',
                                                value: 'Minimize a window by name',
                                                inline: true
                                            }, {
                                                name: '/shiftlock [True/False]',
                                                value: 'Enable or disable shiftlock',
                                                inline: true
                                            }, {
                                                name: '/restart',
                                                value: 'Restart your Computer',
                                                inline: true
                                            }, {
                                                name: '/finditem [item]',
                                                value: 'Find an item in your inventory',
                                                inline: true
                                            }
                                            )
                                            .set_footer(footer => footer
                                                .set_text("Natro Macro")
                                                .set_icon_url("https://images-ext-1.discordapp.net/external/DsQbewHGLgTcm5Y4zLygjiBeEdh1RhljcTnNoNDEHJM/%3Fsize%3D1024/https/cdn.discordapp.com/icons/1012610056921038868/a_f917c2351e5226c7c4f61edfb4e11927.gif?width=461&height=461")
                                            )
                                        ]
                                    })
                                case "priority":
                                    interaction.Reply({
                                        embeds: [
                                            EmbedBuilder()
                                            .set_title("Priority List")
                                            .set_color(0x7380FF)
                                            .set_description("To change the priority list, use the following command:`n```````n/settings set priorityListNumeric [numbers|default]`n```````nEach digit represents its slot in the default priority list.`nFor example:`n```````n/settings set priorityListNumeric 12345678`n```````n`n**Default Priority List**``````ansi`n1 - Night`n2 - Mondo`n3 - Planter`n4 - Bugrun`n5 - Collect`n6 - Quest Rotate`n7 - Boost`n8 - Go Gather``````")
                                        ]
                                    })

                                case "settings":
                                    sections := Map("Boost", "**__Boost__**"
                                        , "Collect", "**__Collect__**"
                                        , "Gather", "**__Gather__**"
                                        , "Planters", "**__Planters__**"
                                        , "Quests", "**__Quests__**"
                                        , "Settings", "**__Settings__**"
                                        , "Status", "**__Status__**"
                                        , "Blender", "**__Blender__**"
                                        , "Shrine", "**__Shrine__**"), sections.Default := ""
                                    for k, v in settings
                                        if HasProp(v, "regex")
                                            sections[v.section] .= "`n" k
                                    enum := sections.__Enum(), enum.Call(,&section)
                                    embeds := [], embed := EmbedBuilder().set_title("List of Settings")
                                    loop 10 {
                                        embed.set_color(0x7380FF), embed.set_description(section)
                                        loop sections.Count {
                                            if (enum.Call(,&section) = 0) {
                                                embeds.Push(embed)
                                                break 2
                                            }
                                            if (StrLen(embed.description) + StrLen(section) > 4092)
                                                break
                                            embed.description.="`n`n" section
                                        }
                                        embeds.Push(embed.Clone()), embed := EmbedBuilder()
                                    }
                                    loop 10 {
                                        FileAppend(StrLen(embeds[A_Index].description) . "`n", "*")
                                    }
                                    msgbox interaction.Reply({
                                        embeds: embeds
                                    })

                            }


                        case "click":
                            x := interaction.getIntegerOption("x"), y := interaction.getIntegerOption("y"), amount := interaction.getIntegerOption("amount").unwrap_or(1), mode := interaction.getStringOption("mode").unwrap_or("Screen")
                            switch mode, 0 {
                                case "Screen", "Window", "Client":
                                CoordMode 'Pixel', mode
                                case "Relative":
                                MouseGetPos(&current_x, &current_y)
                                x := current_x + x, y := current_y + y
                            }
